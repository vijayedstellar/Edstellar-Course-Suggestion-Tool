{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": { "cache-control": "max-age=31536000" }
    },
    {
      "src": "/(.*\\.(js|css|ico|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot))",
      "headers": { "cache-control": "max-age=31536000" }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "headers": { "cache-control": "no-cache" }
    }
  ]
}